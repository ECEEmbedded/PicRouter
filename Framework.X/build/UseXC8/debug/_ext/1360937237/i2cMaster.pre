while(RSEN);
}

# 58
void I2CAck(){
ACKDT = 0;
ACKEN = 1;
while(ACKEN);
}

# 70
void I2CNak(){
ACKDT = 1;
ACKEN = 1;
while(ACKEN);
}

# 82
void I2CWait(){
while ( ( SSPCON2 & 0x1F ) || ( SSPSTAT & 0x04 ) );

}

# 94
void I2CSend(unsigned char dat){
SSPBUF = dat;
while(BF);
I2CWait();
}

# 106
unsigned char I2CRead(void){
unsigned char temp;

RCEN = 1;
while(!BF);
temp = SSPBUF;
I2CWait();
return temp;
}

