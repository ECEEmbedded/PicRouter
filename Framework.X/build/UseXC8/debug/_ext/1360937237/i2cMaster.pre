# 9 "../src/i2cMaster.c"
void I2CInit(void){
TRISC3 = 1;
TRISC4 = 1;
SSPSTAT |= 0x80;
SSPCON = 0x28;
SSPADD = 0x28
}

# 23
void I2CStart(){
SEN = 1;
while(SEN);

}

# 35
void I2CStop(){
PEN = 1;
while(PEN);

}

# 47
void I2CRestart(){
